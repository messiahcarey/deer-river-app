# Production Environment Variables
# Copy this to .env.local for local development with production database

# Database (replace with your AWS RDS connection string)
DATABASE_URL="postgresql://postgres:your_password@your-rds-endpoint:5432/deerriver"

# NextAuth (for future authentication features)
NEXTAUTH_SECRET="your-random-secret-key-here"
NEXTAUTH_URL="https://your-app.amplifyapp.com"

# Optional: Analytics and monitoring
NEXT_PUBLIC_APP_URL="https://your-app.amplifyapp.com"
